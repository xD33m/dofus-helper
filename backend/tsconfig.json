{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"target": "es6",
		"noImplicitAny": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"lib": ["esnext"]
	},
	"exclude": ["node_modules", "dist"],
	"include": ["./**/*.ts"]
}